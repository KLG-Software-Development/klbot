<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>KLG.KLBot.ModuleCollection</PackageId>
    <Authors>KLG Science</Authors>
    <Product>KLBotModuleCollection</Product>
    <Description>一个KLBot模块合集</Description>
    <NeutralLanguage>zh-CN</NeutralLanguage>
    <AssemblyVersion>0.4.*</AssemblyVersion>
    <FileVersion>0.4</FileVersion>
    <Deterministic>False</Deterministic>
    <RootNamespace>ModuleCollection</RootNamespace>
    <Platforms>AnyCPU;x64</Platforms>
    <Version>0.4.0</Version>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <AnalysisLevel>6.0</AnalysisLevel>
    <ProduceReferenceAssembly>True</ProduceReferenceAssembly>
    <TieredCompilation>True</TieredCompilation>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\klbotlib\klbotlib.csproj" />
  </ItemGroup>
</Project>
